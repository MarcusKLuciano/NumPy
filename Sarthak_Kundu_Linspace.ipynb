      "source": [
        "def linspace (num1,num2,num3):\n",
        "  diff= num2-num1\n",
        "  i=1\n",
        "  totval=num3-1\n",
        "  interval=diff/totval\n",
        "  print(num1)\n",
        "  num=num1\n",
        "  while(i<=totval):\n",
        "    print(num+interval)\n",
        "    num=num+interval\n",
        "    i=i+1"
      ],
      "execution_count": 0,
